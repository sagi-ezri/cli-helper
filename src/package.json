{
    "name": "cli-helper",
    "displayName": "CLI Helper",
    "description": "A VSCode extension powered by OpenAI to optimize CLI commands.",
    "version": "0.1.0",
    "publisher": "your-name-or-organization",
    "engines": {
        "vscode": "^1.78.0"
    },
    "categories": [
        "Other",
        "Productivity",
        "AI"
    ],
    "activationEvents": [
        "onCommand:cli-helper.analyzeHistory"
    ],
    "main": "./out/extension.js",
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -p ./",
        "watch": "tsc -w -p ./"
    },
    "devDependencies": {
        "@types/node": "^18.0.0",
        "@types/vscode": "^1.78.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "node-fetch": "^3.0.0"
    },
    "contributes": {
        "commands": [
            {
                "command": "cli-helper.analyzeHistory",
                "title": "Analyze CLI History",
                "category": "CLI Helper"
            }
        ]
    }
}
